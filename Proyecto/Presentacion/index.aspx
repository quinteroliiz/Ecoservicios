<body class="pace-top">
	<!-- begin #page-loader -->
	<div id="page-loader" class="fade in"><span class="spinner"></span></div>
	<!-- end #page-loader -->
	
	<!-- begin #page-container -->
	<div id="page-container" class="fade">
	    <!-- begin login -->
        <div class="login bg-black animated fadeInDown">
            <!-- begin brand -->
            <div class="login-header">
                <div class="brand">
                    <span class="logo"></span> Ecoservicios
                    <small>responsive bootstrap 3 admin template</small>
                </div>
                <div class="icon">
                    <i class="fa fa-sign-in"></i>
                </div>
            </div>
            <!-- end brand -->
            <div class="login-content">
                <form  runat = "server" actionclass=" margin-bottom-0">
                    <div class="form-group m-b-20">
                        <asp:TextBox ID="TB_email" runat="server" placeholder="Email Address" CssClass="form-control input-lg inverse-mode no-border"></asp:TextBox>
                        <asp:RequiredFieldValidator ID="RFV_TB_email" runat="server" ControlToValidate="TB_email" ErrorMessage="El campo email es obligatorio"></asp:RequiredFieldValidator>
                     </div>
                    <div class="form-group m-b-20">
                        <asp:TextBox ID="TB_pass" runat="server" placeholder="Password" CssClass="form-control input-lg inverse-mode no-border" TextMode="Password"></asp:TextBox>
                        <asp:RequiredFieldValidator ID="RFV_TB_pass" runat="server" ControlToValidate="TB_pass" ErrorMessage="El campo contraseña es obligatorio"></asp:RequiredFieldValidator>
                    </div>
                      <div class="login-buttons">
                            <asp:Button ID="B_ingresar" runat="server" Text="Ingresar" CssClass="btn btn-success btn-block btn-lg" OnClick="B_ingresar_Click" />
                
                    </div>
                </form>
            </div>
        </div>
        <!-- end login -->
        
  
	</div>
	<!-- end page container -->
	
